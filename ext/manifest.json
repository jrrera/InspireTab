{
  "name": "InspireTab",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Useful, customizable inspiration and productivity.",
  "homepage_url": "http://jonathanguerrera.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "components/bg/background.html",
    "persistent": true
  },
  "permissions": [
    "management",
    "notifications",
    "tabs"
  ]
}
